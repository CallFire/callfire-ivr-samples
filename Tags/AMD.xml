<dialplan name="Root">
	<amd>
		<live>
			<goto>Main</goto>
		</live>
		<machine>
			<play type="tts">This is the answering machine message.</play>
			<hangup/>
		</machine>
	</amd>
	<menu maxDigits="1" timeout="3500" name="Main">
		<play type="tts">This is the live answer message. Press 1 to transfer to the office.</play>
		<keypress pressed="1">
			<transfer>5551234567</transfer>
		</keypress>
	</menu>
</dialplan>
